<h1>Validar login</h1>

<%= form_for @login, url: validate_path do |form| %>

  <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email, id: :login_email %>
  </div>

  <div class="field">
    <%= form.label :image %>
    <%= form.file_field :image, id: :login_image, multiple: false, accept:'image/png', class: 'form-control' %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
