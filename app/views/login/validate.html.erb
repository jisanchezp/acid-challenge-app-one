<h1>Validar login</h1>

<%= form_for @login, url: validate_path do |form| %>

  <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email, id: :login_email %>
  </div>

  <div class="field">
    <%= form.label :image %>
    <%= form.text_field :image, id: :login_image %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>

